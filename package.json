{
  "name": "purge-lambda-versions",
  "version": "1.0.3",
  "description": "API to delete old AWS Lambda versions",
  "main": "script.js",
  "scripts": {
    "build-version": "npm --no-git-tag-version version patch",
    "create-tag": "git tag v$(node -pe \"require('./package.json')['version']\")",
    "publish-version": "npm publish",
    "deploy": "npm run build-version && npm run create-tag && npm run publish-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulodrade/purge-lambda-versions.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "lambda",
    "versions"
  ],
  "author": "paulodrade",
  "license": "ISC",
  "bin": {
    "plv": "./cli.js"
  },
  "bugs": {
    "url": "https://github.com/paulodrade/purge-lambda-versions/issues"
  },
  "homepage": "https://github.com/paulodrade/purge-lambda-versions#readme",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "async": "^3.2.4",
    "aws-sdk": "^2.1156.0"
  }
}
